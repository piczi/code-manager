{
  "manifest_version": 3,
  "name": "Code Snippet Manager",
  "version": "1.0.0",
  "description": "A powerful code snippet manager for developers",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icons/icon.svg"
    }
  },
  "icons": {
    "128": "icons/icon.svg"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
} 